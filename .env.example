# GraphQL endpoint used server-side (and by the /api/graphql-proxy route)
ALCARRITO_GRAPHQL_URL=https://mcstaging.alcarrito.com/graphql

# REST API base URL used server-side
ALCARRITO_REST_BASE_URL=https://mcstaging.alcarrito.com/rest/V1

# Optional: Magento store-code header sent with every GraphQL request
ALCARRITO_STORE_CODE=compraleacordoba

# Expose GraphQL URL to the browser
NEXT_PUBLIC_ALCARRITO_GRAPHQL_URL=https://mcstaging.alcarrito.com/graphql

# Expose REST base URL to the browser
NEXT_PUBLIC_ALCARRITO_REST_BASE_URL=https://mcstaging.alcarrito.com/rest/V1

# Expose store-code to the browser (used by graphqlClient for the Store header)
NEXT_PUBLIC_ALCARRITO_STORE_CODE=compraleacordoba

# Internal URL for server-side requests to the Next.js graphql-proxy route
# (defaults to http://localhost:3000/api/graphql-proxy when not set)
# INTERNAL_GRAPHQL_PROXY_URL=http://localhost:3000/api/graphql-proxy
